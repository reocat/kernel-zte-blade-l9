#
# sunwave Fingerprint driver configuration
#

config PLATFORM_FINGERPRINT_SUNWAVE
       tristate "generic sunwave fingerprint driver"
       default n
       help
       add support for sunwave fingerprint driver.

config PLATFORM_FINGERPRINT_SUNWAVE_PWR_MODE_REGULATOR
       bool "Sunwave Fingerprint power regulator"
       default n
       help
       SUNWAVECORP Fingerprint power regulator, default GPIO

config FP_PWR_MODE_REGULATOR_VDDCAMMOT
       bool "sunwave Fingerprint power regulator vddcammot"
       default n
       help
       SUNWave Fingerprint power regulator, default GPIO

config SUNWAVE_NAV_REVERT
       bool "sunwave Fingerprint nav revert"
       default n
       help
       SUNWAVE Fingerprint nav revert
